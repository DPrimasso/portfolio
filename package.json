{
  "name": "portfolio",
  "version": "1.0.0",
  "description": "My personal portfolio website",
  "main": "public/index.html",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx --fix",
    "lint:check": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx",
    "format": "prettier --write \"src/**/*.{js,ts,vue,json,css,scss,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,ts,vue,json,css,scss,md}\"",
    "type-check": "vue-tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:coverage:check": "vitest --coverage --reporter=verbose",
    "prepare": "husky install"
  },
  "dependencies": {
    "@vueuse/core": "^14.2.0",
    "@vueuse/head": "^2.0.0",
    "axios": "^1.9.0",
    "bootstrap": "^5.3.0",
    "bootstrap-icons": "^1.10.5",
    "pinia": "^3.0.4",
    "vue": "^3.2.0",
    "vue3-carousel": "^0.15.1",
    "vue3-tour": "^1.0.3"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/vue": "^8.1.0",
    "@types/node": "^20.11.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.23.0",
    "husky": "^9.0.0",
    "jsdom": "^28.0.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "typescript": "^5.3.0",
    "vite": "^4.0.0",
    "vite-imagetools": "^6.3.0",
    "vite-plugin-pwa": "^0.19.0",
    "vitest": "^4.0.18",
    "vue-tsc": "^1.8.0",
    "zod": "^3.22.0"
  }
}
